{
  "name": "pkg-name",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.4.0",
  "description": "pkg-name",
  "author": "LoTwT <l709937065@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/pkg-name.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/pkg-name/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "prettier": "prettier --write .",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "up": "taze latest -I -f",
    "watch": "tsup --watch"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^1.4.2",
    "@ayingott/prettier-config": "^1.2.2",
    "@types/node": "^20.14.9",
    "bumpp": "^9.4.1",
    "eslint": "^9.5.0",
    "eslint-plugin-format": "^0.1.2",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.8",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2",
    "unplugin-auto-import": "^0.17.6",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "prettier": "@ayingott/prettier-config"
}
