{
  "name": "pkg-name",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.6.5",
  "description": "pkg-name",
  "author": "LoTwT <l709937065@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/pkg-name.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/pkg-name/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "prettier": "prettier --write .",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "up": "taze latest -I -f"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^2.0.1",
    "@ayingott/prettier-config": "^2.1.2",
    "@types/node": "^22.13.11",
    "bumpp": "^10.1.0",
    "eslint": "^9.23.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "simple-git-hooks": "^2.12.1",
    "taze": "^19.0.2",
    "tsdown": "^0.6.9",
    "typescript": "^5.8.2",
    "unplugin-auto-import": "^19.1.1",
    "vitest": "^3.0.9"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "simple-git-hooks"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "prettier": "@ayingott/prettier-config"
}
