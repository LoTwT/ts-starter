{
  "name": "{{name}}",
  "version": "0.0.0",
  "packageManager": "pnpm@7.1.2",
  "description": "",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild",
    "prettier": "prettier --write .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && npm publish",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/{{name}}.git"
  },
  "keywords": [],
  "author": "LoTwT <709937065@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LoTwT/{{name}}/issues"
  },
  "homepage": "https://github.com/LoTwT/{{name}}#readme",
  "devDependencies": {
    "@types/node": "^17.0.35",
    "bumpp": "^7.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "pnpm": "^7.1.2",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "unplugin-auto-import": "^0.7.2",
    "vitest": "^0.12.7"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write"
    ]
  }
}
