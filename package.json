{
  "name": "pkg-name",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.14.0",
  "description": "pkg-name",
  "author": "LoTwT <l709937065@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/pkg-name.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/pkg-name/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "prettier": "prettier --write .",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "up": "taze latest -I -f"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^2.0.5",
    "@ayingott/prettier-config": "^2.1.5",
    "@types/node": "^24.3.0",
    "bumpp": "^10.2.3",
    "eslint": "^9.33.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "simple-git-hooks": "^2.13.1",
    "taze": "^19.1.0",
    "tsdown": "^0.14.1",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^20.0.0",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "prettier": "@ayingott/prettier-config"
}
